global function Boolean OtherExchangeRate(val fr,val to1,val to2,val br1,val br2,val afr,val ato1,val ato2,val abr1,val abr2)
begin
  Boolean res;
  
  res = false;
  if ((afr!=fr) or (ato1!=to1) or (ato2!=to2) or (abr1!=br1) or (abr2!=br2)) then begin
    res = true;
  end;
  OtherExchangeRate = res;
  return;
end;
