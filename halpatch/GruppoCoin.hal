NCodeVc CoinEANCoder;  record CoinItemListVc CoinItemListr;    switch (RepSpec.flags[1]) begin    case 0:      CoinPackListr.SerNr = -1;      while (LoopMain(CoinPackListr,1,true)) begin        RecordDelete(CoinPackListr);        StepBack(CoinPackListr);      end;    case 1:      CoinEANCoder.SerNr = -1;      while (LoopMain(CoinEANCoder,1,true)) begin        RecordDelete(CoinEANCoder);        StepBack(CoinEANCoder);      end;    case 2:      CoinItemListr.SerNr = -1;      while (LoopMain(CoinItemListr,1,true)) begin        RecordDelete(CoinItemListr);        StepBack(CoinItemListr);      end;  end;  return;end;