external procedure ExtractObj(string,var Integer,var string);global procedure ItemSaldoRClassReportDefaults(integer wn)begin	record RcVc RepSpec;	record UserVc User;	integer i;	string 20 loc;		getwindowrecord(wn,RepSpec);	User.Code = currentuser;	if(readfirstmain(User,1,true))then begin		if(nonblank(User.UserLocations) and usercanaction("AllowReportWithUserLoc",false))then begin			i = 0;			ExtractObj(User.UserLocations,i,loc);			RepSpec.f1 = loc;			putwindowrecord(wn,RepSpec);		end;	end;return;end;