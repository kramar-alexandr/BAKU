  PutWindowRecord(wn,RepSpec);
  SelectWindow(wn);
  
  RETURN;
END;

procedure PMArrayMaker(var integer cnt,var integer maxcnt, var array string pm, string paymode)
begin
integer i;
Boolean TrHs;

  i = 0;
  TrHs = true;
  while(TrHs) begin
    if TrHs then begin
      if nonblank(pm[i]) then begin
        if pm[i]==paymode then begin
        cnt = i;
        TrHs = false;
        end;
      end else begin
        pm[i] = paymode;
        cnt = i;
        TrHs = false;
      end;
      i = i+1;
      if i>maxcnt then begin
        maxcnt = i;
      end;
    end;
  end;

RETURN;
END;

procedure SMArrayMaker(var integer cnt,var integer maxcnt, var array string sm, string salesman)
begin
integer i;
Boolean TrHs;
  i = 0;
  TrHs = true;
  while(TrHs) begin
    if TrHs then begin
      if nonblank(sm[i]) then begin
        if sm[i]==salesman then begin
        cnt = i;
        TrHs = false;
        end;
      end else begin
        sm[i] = salesman;
        cnt = i;
        TrHs = false;
      end;
      i = i+1;
      if i>maxcnt then begin
        maxcnt = i;
      end;
    end;
  end;

RETURN;
END;

